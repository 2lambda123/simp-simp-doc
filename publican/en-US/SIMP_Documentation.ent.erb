<!-- The current document SIMP Version -->
<!ENTITY SIMP_VERSION "<%= doc_product_number[:version] %>">
<!-- The current document SIMP Release -->
<!ENTITY SIMP_RELEASE "<%= doc_product_number[:release] %>">
<!-- The current document RHEL Versions -->
<!ENTITY RHEL_VERSION "<%= args.os_version %>">
<!ENTITY RHEL_MAJOR_VERSION "<%= args.os_version.split('.').first %>">
<!ENTITY RHEL_MAJOR_MINOR_VERSION "<%= args.os_version.split('.')[0..1].join('.') %>">
<!-- The generic RHEL 5 Version -->
<!ENTITY RHEL_5_VERSION "5.8">
<!-- The generic RHEL 6 Version -->
<!ENTITY RHEL_6_VERSION "6.3">
